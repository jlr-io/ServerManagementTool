<p id="notice"><%= notice %></p>
<h1>Unaccepted Tickets:</h1>
<%= form_tag unaccepted_path, class: 'searchbox', :method => 'get' do %>
  <%= text_field_tag :search, params[:search], placeholder: "Enter search params..."%>
  <%= submit_tag "Search" %>
<% end %>

<div id=" unaccepted results">
  <%= render 'results_unaccepted' %>
</div>