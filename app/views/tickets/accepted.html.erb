<p id="notice"><%= notice %></p>
<h1>Accepted Tickets:</h1>
<%= form_tag accepted_path, class: 'searchbox', :method => 'get' do %>
  <%= text_field_tag :search, params[:search], placeholder: "Enter search params..."%>
  <%= submit_tag "Search" %>
<% end %>

<div id=" accepted results">
  <%= render 'results_accepted' %>
</div>